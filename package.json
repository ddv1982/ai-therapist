    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.9.1",
    "swagger-typescript-api": "^13.2.10",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "^5.6.0",
    "web-streams-polyfill": "^4.2.0"
  },
  "optionalDependencies": {}
}
