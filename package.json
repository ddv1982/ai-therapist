{
  "name": "therapist-ai-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo --port 4000",
    "dev:local": "next dev --turbo --hostname 127.0.0.1 --port 4000",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 --port 4000",
    "start:local": "next start --hostname localhost --port 4000",
    "lint": "eslint .",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "api:types": "openapi-typescript docs/api.yaml -o src/types/api.generated.ts",
    "encryption:generate": "node scripts/setup-encryption.js generate",
    "encryption:setup": "node scripts/setup-encryption.js setup",
    "encryption:validate": "node scripts/setup-encryption.js validate",
    "env:init": "node scripts/setup-env-local.js",
    "totp": "npx tsx scripts/totp-manager.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "qa:smoke": "npm run api:types && npm run lint && tsc --noEmit && npm test",
    "qa:full": "npm run qa:smoke && npm run test:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/react": "^2.0.76",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-table": "^8.20.5",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.10",
    "@types/redux-persist": "^4.0.0",
    "@types/speakeasy": "^2.0.10",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "ai": "^5.0.76",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "jose": "^6.1.0",
    "lucide-react": "^0.542.0",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "next": "^15.5.2",
    "next-intl": "^4.3.6",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
